  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug on DEV",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnFirstSessionStart",
      // "program": "${workspaceFolder}/src/shared/index.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "active"
      }
    }
  ]
}
